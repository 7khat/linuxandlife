<!DOCTYPE html>
<html>
<head>
  <%- @getBlock("styles").add(["/style/style.css"]).toHTML() %>
  <%- @getBlock("meta").toHTML() %>
</head>

<body>
  <%- @getBlock("scripts").toHTML() %>

  <table align=left border=1 width=300px ><tr><td>
    <h1>فهرست کتاب</h1>
    <ul>
    <% for article in @getCollection("html").findAllLive({relativeOutDirPath: "articles"}).toJSON(): %>
    <li><a href=<%- article.url %>><%= article.title %></a></li>
    <% end %>
    </ul>
  </td></tr></table>

  
  <%- @content %>

</body>
</html>